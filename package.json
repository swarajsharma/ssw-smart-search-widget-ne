{
  "name": "nenb-no",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "npm run serve:nenb-no:dev",
    "serve:nenb-no:dev": "ng serve nenb-no --browserTarget nenb-no:build:dev",
    "serve:necbe-nl:dev": "ng serve necbe-nl --browserTarget necbe-nl:build:dev",
    "serve:necbe-fr:dev": "ng serve necbe-fr --browserTarget necbe-fr:build:dev",
    "serve:necnl:dev": "ng serve necnl --browserTarget necnl:build:dev",
    "build": "npm run build:all",
    "build:nenb-no": "ng build nenb-no --configuration prod --stats-json",
    "build:necbe-nl": "ng build necbe-nl --configuration prod --stats-json",
    "build:necbe-fr": "ng build necbe-fr --configuration prod --stats-json",
    "build:necnl": "ng build necnl --configuration prod --stats-json",
    "build:all": "npm run build:nenb-no && npm run build:necbe-nl && npm run build:necbe-fr && npm run build:necnl && node getAssets.js",
    "test": "npm install && npm run test:nocoverage",
    "test:coverage": "ng test nenb-no --configuration dev --watch=false --code-coverage",
    "test:nocoverage": "ng test nenb-no --configuration dev --watch=false",
    "test:watch": "ng test nenb-no --configuration dev --watch=true",
    "lint": "./node_modules/.bin/sass-lint -v -q && ng lint --type-check",
    "e2e": "ng e2e",
    "storybook": "start-storybook -p 4201 -c .storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/cdk": "^7.2.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.2.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ngrx/effects": "^7.0.0",
    "@ngrx/router-store": "^7.0.0",
    "@ngrx/store": "^7.0.0",
    "@ngrx/store-devtools": "^7.0.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@storybook/angular": "^4.1.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "date-fns": "^1.30.1",
    "hammerjs": "^2.0.8",
    "ngx-daterangepicker-material": "^1.2.7",
    "ngx-translate-parser-plural-select": "^1.1.3",
    "node-sass": "^4.11.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "url-search-params-polyfill": "^5.0.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.0",
    "typescript": "^3.1.1"
  }
}
